{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "dev": {
      "dependsOn": ["db:generate"],
      "cache": false
    },
    "build": {
      "dependsOn": ["^build", "db:generate"],
      "env": ["SOME_ENV_VAR"],
      "outputs": ["dist/**"]
    },
    "web#build": {
      "dependsOn": ["^build", "db:generate"],
      "env": ["SOME_OTHER_ENV_VAR"]
    },
    "outputs": [".next/**"],
    "globalEnv": ["GITHUB_TOKEN"],
    "globalDependencies": ["tsconfig.json"],
    "db:generate": {
      "cache": false
    },
    "db:push": {
      "cache": false
    },
    "db:migrate:deploy": {
      "outputs": []
    },
    "db:seed": {
      "outputs": []
    },
    "lint": {
      "outputs": []
    },
    "test": {
      "dependsOn": ["build"],
      "inputs": [
        "src/**/*.tsx",
        "src/**/*.ts",
        "test/**/*.ts",
        "test/**/*.tsx"
      ],
      "outputs": []
    },
    "test:watch": {
      "dependsOn": ["build"],
      "inputs": [
        "src/**/*.tsx",
        "src/**/*.ts",
        "test/**/*.ts",
        "test/**/*.tsx"
      ],
      "outputs": []
    },
    "clean": {
      "cache": false
    },
    "deploy": {
      "dependsOn": ["build", "test", "lint"]
    }
  }
}
