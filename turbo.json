{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "dev": {
      "dependsOn": ["db:generate"],
      "cache": false
    },
    "web#build": {
      "dependsOn": [
        "^build",
        "db:generate",
        "common#build",
        "database#build",
        "ui#build"
      ],
      "outputs": []
    },
    "worker#build": {
      "dependsOn": ["common#build", "database#build"],
      "outputs": []
    },
    "common#build": {
      "outputs": []
    },
    "database#build": {
      "dependsOn": ["common#build"],
      "outputs": []
    },
    "ui#build": {
      "dependsOn": ["common#build"],
      "outputs": []
    },
    "build": {
      "dependsOn": ["common#build", "database#build", "ui#build"],
      "env": ["SOME_ENV_VAR"],
      "outputs": ["dist/**"]
    },
    "db:generate": {
      "cache": false
    },
    "db:push": {
      "cache": false
    },
    "db:migrate:deploy": {
      "outputs": []
    },
    "db:seed": {
      "outputs": []
    },
    "lint": {
      "outputs": []
    },
    "test": {
      "dependsOn": ["^build"],
      "inputs": [
        "src/**/*.tsx",
        "src/**/*.ts",
        "test/**/*.ts",
        "test/**/*.tsx"
      ],
      "outputs": []
    },
    "test:watch": {
      "dependsOn": ["^build"],
      "inputs": [
        "src/**/*.tsx",
        "src/**/*.ts",
        "test/**/*.ts",
        "test/**/*.tsx"
      ],
      "outputs": []
    },
    "clean": {
      "cache": false
    },
    "deploy": {
      "dependsOn": ["^build", "test", "lint"]
    }
  }
}
