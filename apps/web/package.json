{
	"name": "web",
	"version": "1.0.4",
	"private": true,
	"scripts": {
		"ts-node": "ts-node --compiler-options \"{\\\"module\\\":\\\"commonjs\\\"}\"",
		"test": "jest --passWithNoTests",
		"test:watch": "yarn test --watchAll",
		"test:ci": "yarn test --ci --coverage --maxWorkers=2",
		"test:unit": "yarn test",
		"test:e2e": "playwright test",
		"setup": "run-s deploy-db migrate-db seed-db",
		"docker-use-remote": "docker context use home-server",
		"deploy-db": "docker-compose up -d postgres postgres-backup",
		"deploy:app": "docker-compose up -d nextjs",
		"seed-db": "prisma db seed",
		"prisma-generate": "prisma generate",
		"prisma-format": "prisma format",
		"migrate-db": "prisma migrate dev",
		"prisma-build": "run-s prisma-format prisma-generate migrate-db",
		"prisma-studio": "prisma studio",
		"reset-db": "docker-compose down --volumes --remove-orphans",
		"postinstall": "prisma generate",
		"commit": "cz",
		"pre-commit": "lint-staged",
		"prebuild": "prisma generate",
		"build": "next build",
		"postbuild": "next-sitemap --config next-sitemap.config.js",
		"dev": "next dev -p 3000",
		"start": "next start",
		"type-check": "tsc --noEmit",
		"format": "prettier --write .",
		"lint": "next lint",
		"lint:fix": "eslint . --fix",
		"up": "yarn upgrade-interactive",
		"up-latest": "yarn up --latest",
		"release": "cross-env HUSKY=0 standard-version",
		"push-release": "git push --follow-tags origin main",
		"swagger": "next-swagger-doc-cli next-swagger-doc.json"
	},
	"browserslist": {
		"production": [
			">0.3%",
			"not ie 11",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"jest": {
		"moduleDirectories": [
			"node_modules",
			"src"
		],
		"moduleFileExtensions": [
			"js",
			"json",
			"ts"
		],
		"moduleNameMapper": {
			"~/(.*)": "<rootDir>/$1"
		},
		"roots": [
			"./"
		],
		"testEnvironment": "node",
		"testRegex": ".test.ts$",
		"transform": {
			"^.+\\.(t|j)s$": "ts-jest"
		}
	},
