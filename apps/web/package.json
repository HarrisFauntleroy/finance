{
  "name": "web",
  "version": "2.0.0",
  "private": true,
  "homepage": "https://elixir-money-monorepo-web.vercel.app/",
  "repository": {
    "type": "git",
    "url": "https://github.com/HarrisFauntleroy/elixir.money-monorepo"
  },
  "license": "MIT",
  "author": {
    "name": "Harris Fauntleroy",
    "url": "https://github.com/HarrisFauntleroy"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "next build",
    "postbuild": "next-sitemap --config next-sitemap.config.js",
    "build-storybook": "storybook build",
    "clean": "rimraf .turbo node_modules dist tsconfig.tsbuildinfo",
    "dev": "next dev -p 3000",
    "format": "prettier --write .",
    "lint": "next lint",
    "lint:fix": "eslint . --fix && yarn format",
    "lint:watch": "yarn esw . --ext .js,.jsx,.ts,.tsx --watch --fix --color --changed --clear",
    "reset-db": "docker-compose down --volumes --remove-orphans",
    "start": "next start",
    "storybook": "storybook dev -p 6007",
    "storybook:build": "storybook build",
    "storybook:serve": "http-server storybook-static --port 6007 --silent",
    "swagger": "next-swagger-doc-cli next-swagger-doc.json",
    "test": "jest --passWithNoTests",
    "test:ci": "yarn test --ci --coverage --maxWorkers=2",
    "test:watch": "yarn test --watchAll",
    "type-check": "tsc --noEmit",
    "watch": "yarn type-check --watch"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "yarn lint:fix",
      "yarn format"
    ]
  },
  "browserslist": {
    "production": [
      ">0.3%",
      "not ie 11",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "prettier": {
    "arrowParens": "always",
    "bracketSameLine": false,
    "bracketSpacing": true,
    "endOfLine": "lf",
    "htmlWhitespaceSensitivity": "strict",
    "jsxSingleQuote": false,
    "printWidth": 80,
    "proseWrap": "preserve",
    "quoteProps": "as-needed",
    "semi": true,
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "all",
    "useTabs": false,
    "vueIndentScriptAndStyle": false
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "es6": true,
      "jest": true,
      "node": true
    },
    "parser": "@typescript-eslint/parser",
    "settings": {
      "import/resolver": {
        "typescript": true,
        "node": true
      },
      "react": {
        "version": "detect"
      }
    },
    "plugins": [
      "@typescript-eslint",
      "simple-import-sort",
      "import",
      "sonarjs",
      "jsx-a11y",
      "prettier"
    ],
    "extends": [
      "next",
      "plugin:@typescript-eslint/recommended",
      "eslint:recommended",
      "plugin:import/typescript",
      "plugin:import/recommended",
      "plugin:sonarjs/recommended",
      "plugin:jsx-a11y/recommended",
      "plugin:storybook/recommended",
      "plugin:prettier/recommended"
    ],
    "rules": {
      "@typescript-eslint/no-unused-vars": [
        "error",
        {
          "argsIgnorePattern": "^_"
        }
      ],
      "import/extensions": "off",
      "simple-import-sort/imports": [
        "off",
        {
          "groups": [
            [
              "^react$"
            ],
            [
              "^(ui|common|database|database/generated/prisma-client)$"
            ],
            [
              "^[./]"
            ]
          ]
        }
      ],
      "@next/next/no-html-link-for-pages": [
        "error",
        "pages"
      ]
    },
    "ignorePatterns": [
      "dist"
    ]
  },
  "jest": {
    "testEnvironment": "node",
    "testRegex": ".test.*",
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    }
  },
  "resolutions": {
    ".prisma": "link:node-modules/.prisma"
  },
  "dependencies": {
    "@chakra-ui/icons": "2.0.18",
    "@chakra-ui/react": "^2.5.5",
    "@chakra-ui/theme-tools": "2.0.16",
    "@choc-ui/chakra-autocomplete": "5.1.4",
    "@emotion/react": "^11.10.8",
    "@emotion/styled": "^11.10.6",
    "@hookform/resolvers": "^3.0.0",
    "@mantine/carousel": "^6.0.10",
    "@mantine/core": "^6.0.10",
    "@mantine/dates": "^6.0.10",
    "@mantine/dropzone": "^6.0.10",
    "@mantine/form": "^6.0.10",
    "@mantine/hooks": "^6.0.10",
    "@mantine/modals": "^6.0.10",
    "@mantine/notifications": "^6.0.10",
    "@mantine/nprogress": "^6.0.10",
    "@mantine/prism": "^6.0.10",
    "@mantine/spotlight": "^6.0.10",
    "@mantine/tiptap": "^6.0.10",
    "@next-auth/prisma-adapter": "^1.0.4",
    "@next/eslint-plugin-next": "13.3.0",
    "@tabler/icons-react": "^2.17.0",
    "@tanstack/match-sorter-utils": "8.8.4",
    "@tanstack/react-query": "4.29.3",
    "@tanstack/react-query-devtools": "^4.20.2",
    "@tanstack/react-table": "^8.7.3",
    "@tanstack/table-core": "^8.8.5",
    "@tiptap/extension-link": "^2.0.3",
    "@tiptap/react": "^2.0.3",
    "@tiptap/starter-kit": "^2.0.3",
    "@tremor/react": "^2.4.0",
    "@trpc/client": "^10.5.0",
    "@trpc/next": "^10.5.0",
    "@trpc/react": "9.27.4",
    "@trpc/react-query": "^10.5.0",
    "@trpc/server": "^10.5.0",
    "apexcharts": "3.39.0",
    "axios": "1.3.5",
    "chakra-ui-steps": "^1.8.0",
    "chart.js": "3.9.1",
    "common": "*",
    "currency.js": "2.0.4",
    "database": "*",
    "date-fns": "^2.29.3",
    "dayjs": "^1.11.7",
    "embla-carousel-react": "^8.0.0-rc03",
    "framer-motion": "^10.11.2",
    "i18next": "^22.0.4",
    "i18next-browser-languagedetector": "^7.0.1",
    "i18next-http-backend": "^2.0.1",
    "lodash": "^4.17.21",
    "next": "13.3.0",
    "next-auth": "^4.18.7",
    "next-i18next": "^12.1.0",
    "next-seo": "6.0.0",
    "next-swagger-doc": "0.3.6",
    "nodemailer": "6.9.1",
    "papaparse": "^5.3.2",
    "qs": "6.11.1",
    "react": "18.2.0",
    "react-apexcharts": "1.4.0",
    "react-chartjs-2": "4.3.1",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.43.9",
    "react-i18next": "^12.0.0",
    "react-icons": "4.8.0",
    "react-intl": "6.4.1",
    "react-spring": "9.7.1",
    "superjson": "1.12.2",
    "swagger-ui-react": "4.18.2",
    "tslib": "^2.5.0",
    "ui": "*",
    "uuid": "9.0.0",
    "ws": "8.13.0",
    "zod": "3.21.4",
    "zod-prisma-types": "^2.5.6"
  },
  "devDependencies": {
    "@jest/types": "29.5.0",
    "@storybook/addon-essentials": "^7.0.4",
    "@storybook/addon-interactions": "^7.0.4",
    "@storybook/addon-links": "^7.0.4",
    "@storybook/blocks": "^7.0.4",
    "@storybook/nextjs": "^7.0.4",
    "@storybook/react": "^7.0.4",
    "@storybook/testing-library": "^0.0.14-next.2",
    "@types/jest": "^29.5.0",
    "@types/node": "^18.11.6",
    "@types/nodemailer": "6.4.7",
    "@types/papaparse": "^5.3.7",
    "@types/react": "18.0.37",
    "@types/react-dom": "^18.0.11",
    "@types/swagger-ui-react": "4.18.0",
    "@types/uuid": "9.0.1",
    "@types/ws": "^8.5.3",
    "@typescript-eslint/eslint-plugin": "^5.51.0",
    "@typescript-eslint/parser": "^5.58.0",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.38.0",
    "eslint-config-next": "13.3.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-sonarjs": "0.19.0",
    "eslint-plugin-storybook": "^0.6.11",
    "eslint-watch": "^8.0.0",
    "jest": "29.5.0",
    "next-sitemap": "4.0.7",
    "postcss": "^8.4.23",
    "rimraf": "^4.4.1",
    "storybook": "^7.0.4",
    "string-replace-loader": "^3.1.0",
    "tailwindcss": "^3.3.2",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "typescript": "5.0.4"
  },
  "packageManager": "yarn@3.5.0",
  "ct3aMetadata": {
    "initVersion": "6.10.2"
  }
}
