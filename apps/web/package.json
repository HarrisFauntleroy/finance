  "scripts": {
    "build": "next build",
    "postbuild": "next-sitemap",
    "build-storybook": "storybook build",
    "dev": "next dev",
    "reset-db": "docker-compose down --volumes --remove-orphans",
    "start": "next start",
    "storybook": "storybook dev -p 6007",
    "storybook:build": "storybook build",
    "storybook:serve": "http-server storybook-static --port 6007 --silent",
    "swagger": "next-swagger-doc-cli next-swagger-doc.json",
    "test": "jest --passWithNoTests",
    "test-storybook": "test-storybook",
    "test:ci": "yarn test --ci --coverage --maxWorkers=2",
    "test:watch": "yarn test --watchAll",
    "type-check": "tsc --noEmit",
    "watch": "yarn type-check --watch"
  },
  "browserslist": {
    "production": [
      ">0.3%",
      "not ie 11",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "coverageThreshold": {
      "global": {
        "lines": 80
      }
    },
    "testEnvironment": "node",
    "testRegex": ".test.*",
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    }
  },
  "resolutions": {
    ".prisma": "link:node-modules/.prisma"
  },
