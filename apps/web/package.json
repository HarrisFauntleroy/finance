{
  "name": "web",
  "version": "1.0.4",
  "private": true,
  "scripts": {
    "build": "next build",
    "postbuild": "next-sitemap --config next-sitemap.config.js",
    "commit": "cz",
    "deploy-db": "docker-compose up -d postgres postgres-backup",
    "deploy:app": "docker-compose up -d nextjs",
    "dev": "next dev -p 3000",
    "docker-use-remote": "docker context use home-server",
    "format": "prettier --write .",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "pre-commit": "lint-staged",
    "push-release": "git push --follow-tags origin main",
    "release": "cross-env HUSKY=0 standard-version",
    "reset-db": "docker-compose down --volumes --remove-orphans",
    "setup": "run-s deploy-db migrate-db seed-db",
    "start": "next start",
    "swagger": "next-swagger-doc-cli next-swagger-doc.json",
    "test": "jest --passWithNoTests",
    "test:ci": "yarn test --ci --coverage --maxWorkers=2",
    "test:e2e": "playwright test",
    "test:unit": "yarn test",
    "test:watch": "yarn test --watchAll",
    "ts-node": "ts-node --compiler-options \"{\\\"module\\\":\\\"commonjs\\\"}\"",
    "type-check": "tsc --noEmit",
    "up": "yarn upgrade-interactive",
    "up-latest": "yarn up --latest"
  },
  "browserslist": {
    "production": [
      ">0.3%",
      "not ie 11",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "testEnvironment": "node",
    "testRegex": ".test.*",
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    }
  },
  "dependencies": {
