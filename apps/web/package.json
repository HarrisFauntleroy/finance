{
  "name": "web",
  "version": "1.1.0",
  "private": true,
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "next build",
    "postbuild": "next-sitemap --config next-sitemap.config.js",
    "clean": "rimraf .turbo node_modules dist tsconfig.tsbuildinfo",
    "dev": "next dev -p 3000",
    "lint": "yarn eslint .",
    "lint:watch": "yarn esw . --watch --clear --changed",
    "lint:fix": "yarn eslint . --fix",
    "reset-db": "docker-compose down --volumes --remove-orphans",
    "start": "next start",
    "swagger": "next-swagger-doc-cli next-swagger-doc.json",
    "test": "jest --passWithNoTests",
    "test:ci": "yarn test --ci --coverage --maxWorkers=2",
    "test:watch": "yarn test --watchAll",
    "type-check": "tsc --noEmit"
  },
  "browserslist": {
    "production": [
      ">0.3%",
      "not ie 11",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "testEnvironment": "node",
    "testRegex": ".test.*",
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    }
  },
  "dependencies": {
