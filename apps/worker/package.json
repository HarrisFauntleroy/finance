{
  "name": "worker",
  "version": "1.1.0",
  "private": true,
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "clean": "rimraf .turbo node_modules dist tsconfig.tsbuildinfo",
    "dev": "NODE_ENV=development nodemon src/index.ts",
    "lint": "yarn esw . --ext .js,.jsx,.ts,.tsx --color",
    "lint:watch": "yarn esw . --ext .js,.jsx,.ts,.tsx --watch --fix --color --changed --clear",
    "lint:fix": "yarn esw . --ext .js,.jsx,.ts,.tsx --fix --color",
    "start": "NODE_ENV=production node dist/index.js",
    "test": "jest --passWithNoTests",
    "test:ci": "yarn test --ci --coverage --maxWorkers=2",
    "test:watch": "yarn test --watchAll",
    "type-check": "tsc --noEmit"
  },
  "jest": {
    "testEnvironment": "node",
    "testRegex": ".test.*",
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    }
  },
  "dependencies": {
    "@bull-board/express": "^4.8.0",
    "@cosmjs/stargate": "^0.29.5",
    "@cosmjs/tendermint-rpc": "^0.29.5",
    "@harmony-js/core": "^0.1.57",
    "@harmony-js/utils": "^0.1.56",
    "ansi-colors": "^4.1.3",
    "axios": "1.3.5",
    "body-parser": "^1.20.1",
    "bullmq": "^3.4.1",
    "cli-progress": "^3.11.2",
    "common": "*",
    "cosmjs-types": "^0.6.1",
    "database": "*",
    "date-fns": "^2.29.3",
    "dinero.js": "1.9.1",
    "dotenv": "^16.0.3",
    "evmosjs": "^0.2.16",
    "express": "4.18.2",
    "node-fetch": "^3.3.0",
    "progress": "^2.0.3",
    "web3": "^1.8.1",
    "zod": "3.21.4"
  },
  "devDependencies": {
    "@babel/core": "^7.21.4",
    "@babel/preset-env": "^7.21.4",
    "@jest/types": "29.4.0",
    "@types/express": "4.17.15",
    "@types/jest": "^29.5.0",
    "@types/node": "^18.11.6",
    "@typescript-eslint/eslint-plugin": "^5.51.0",
    "@typescript-eslint/parser": "^5.58.0",
    "babel-jest": "^29.5.0",
    "eslint": "^8.38.0",
    "eslint-config-next": "13.2.4",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-sonarjs": "0.16.0",
    "eslint-watch": "^8.0.0",
    "jest": "29.4.3",
    "nodemon": "2.0.22",
    "prisma": "*",
    "rimraf": "^4.4.1",
    "ts-jest": "^29.1.0",
    "ts-node": "10.9.1",
    "typescript": "5.0.4"
  },
  "packageManager": "yarn@3.5.0"
}
