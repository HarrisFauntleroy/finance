{
  "name": "database",
  "version": "2.0.0",
  "private": true,
  "homepage": "",
  "repository": {
    "type": "git",
    "url": "https://github.com/HarrisFauntleroy/alchemical-finance"
  },
  "license": "MIT",
  "author": {
    "name": "Harris Fauntleroy",
    "url": "https://github.com/HarrisFauntleroy"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/**"
  ],
  "scripts": {
    "build": "tsup",
    "clean": "rimraf .turbo node_modules dist",
    "db:migrate:deploy": "prisma migrate deploy",
    "db:migrate:dev": "prisma migrate dev",
    "db:push": "prisma db push",
    "db:seed": "tsx src/seed.ts",
    "dev": "concurrently \"tsup --watch\" \"yarn studio\"",
    "db:generate": "prisma generate --schema=prisma/schema.prisma",
    "prebuild": "yarn db:generate",
    "predev": "yarn db:generate",
    "studio": "prisma studio",
    "test": "jest --passWithNoTests",
    "test:ci": "yarn test --ci --coverage --maxWorkers=2",
    "test:watch": "yarn test --watchAll",
    "type-check": "tsc --noEmit"
  },
  "jest": {
    "coverageThreshold": {
      "global": {
        "lines": 80
      }
    },
    "testEnvironment": "node",
    "testRegex": ".test.*",
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    }
  },
  "dependencies": {
    "@prisma/client": "^4.15.0",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@jest/types": "29.5.0",
    "@types/jest": "^29.5.2",
    "@types/node": "^18.16.18",
    "concurrently": "^8.0.1",
    "jest": "29.5.0",
    "prisma": "^4.15.0",
    "rimraf": "^4.4.1",
    "ts-jest": "^29.1.0",
    "tsup": "^6.7.0",
    "tsx": "^3.12.7",
    "typescript": "5.0.3"
  },
  "packageManager": "yarn@3.8.1",
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  }
}
