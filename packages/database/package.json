{
  "name": "database",
  "version": "1.1.0",
  "private": true,
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prebuild": "yarn generate",
    "build": "tsup",
    "clean": "rimraf .turbo node_modules dist tsconfig.tsbuildinfo",
    "db:migrate:deploy": "prisma migrate deploy",
    "db:migrate:dev": "prisma migrate dev",
    "db:push": "prisma db push --skip-generate",
    "db:seed": "tsx src/seed.ts",
    "predev": "yarn generate",
    "dev": "concurrently \"yarn build --watch\" \"yarn prisma studio\" \"yarn prisma generate --watch\"",
    "generate": "prisma generate",
    "postinstall": "prisma generate --schema=prisma/schema.prisma",
    "lint": "yarn esw . --ext .js,.jsx,.ts,.tsx --color",
    "lint:watch": "yarn esw . --ext .js,.jsx,.ts,.tsx --watch --fix --color --changed --clear",
    "lint:fix": "yarn esw . --ext .js,.jsx,.ts,.tsx --fix --color",
    "prepare": "yarn build",
    "start": "NODE_ENV=production node dist/index.js",
    "test": "jest --passWithNoTests",
    "test:ci": "yarn test --ci --coverage --maxWorkers=2",
    "test:watch": "yarn test --watchAll",
    "type-check": "tsc --noEmit"
  },
  "jest": {
    "testEnvironment": "node",
    "testRegex": ".test.*",
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    }
  },
  "dependencies": {
    "@prisma/client": "^4.1.1",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@jest/types": "29.5.0",
    "@types/jest": "^29.5.0",
    "@types/node": "^18.11.6",
    "@typescript-eslint/eslint-plugin": "^5.51.0",
    "@typescript-eslint/parser": "^5.58.0",
    "concurrently": "^8.0.1",
    "eslint": "^8.38.0",
    "eslint-config-next": "13.3.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-sonarjs": "0.19.0",
    "eslint-watch": "^8.0.0",
    "jest": "29.5.0",
    "prisma": "^4.1.1",
    "rimraf": "^4.4.1",
    "ts-jest": "^29.1.0",
    "tsup": "^6.7.0",
    "tsx": "^3.7.1",
    "typescript": "5.0.4"
  },
  "packageManager": "yarn@3.5.0"
}
