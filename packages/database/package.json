{
  "name": "database",
  "version": "2.0.0",
  "private": true,
  "homepage": "",
  "repository": {
    "type": "git",
    "url": "https://github.com/HarrisFauntleroy/alchemical-finance"
  },
  "license": "MIT",
  "author": {
    "name": "Harris Fauntleroy",
    "url": "https://github.com/HarrisFauntleroy"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prebuild": "yarn generate",
    "build": "tsup",
    "db:migrate:deploy": "prisma migrate deploy",
    "db:generate": "prisma generate",
    "db:push": "prisma db push --skip-generate",
    "db:seed": "prisma db seed",
    "predev": "yarn generate",
    "dev": "concurrently \"yarn build --watch\" \"yarn prisma studio\" \"yarn generate --watch\"",
    "generate": "prisma generate --schema=prisma/schema.prisma",
    "postinstall": "yarn generate",
    "prepare": "yarn build",
    "start": "NODE_ENV=production node dist/index.js",
    "test": "jest --passWithNoTests",
    "test:ci": "yarn test --ci --coverage --maxWorkers=2",
    "test:watch": "yarn test --watchAll",
    "type-check": "tsc --noEmit",
    "watch": "yarn type-check --watch"
  },
  "jest": {
    "coverageThreshold": {
      "global": {
        "lines": 80
      }
    },
    "testEnvironment": "node",
    "testRegex": ".test.*",
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    }
  },
  "dependencies": {
    "@prisma/client": "^4.15.0",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@jest/types": "29.5.0",
    "@types/jest": "^29.5.2",
    "@types/node": "^18.16.17",
    "concurrently": "^8.0.1",
    "jest": "29.5.0",
    "prisma": "^4.15.0",
    "rimraf": "^4.4.1",
    "ts-jest": "^29.1.0",
    "tsup": "^6.7.0",
    "tsx": "^3.12.7",
    "typescript": "5.1.3"
  },
  "packageManager": "yarn@3.5.1",
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  }
}
