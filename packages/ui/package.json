{
  "name": "ui",
  "version": "1.0.1",
  "private": true,
  "license": "MIT",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "build-storybook": "build-storybook",
    "clean": "rimraf .turbo node_modules dist tsconfig.tsbuildinfo",
    "dev": "yarn build -- --watch",
    "format": "prettier --write .",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "preview": "vite preview",
    "start": "NODE_ENV=production ts-node dist/index.js",
    "storybook": "start-storybook -p 6006",
    "test": "jest --passWithNoTests",
    "test:ci": "yarn test --ci --coverage --maxWorkers=2",
    "test:watch": "yarn test --watchAll",
    "type-check": "tsc --noEmit"
  },
  "jest": {
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "roots": [
      "src"
    ],
    "testEnvironment": "node",
    "testRegex": ".test.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    }
  },
  "dependencies": {
    "@chakra-ui/react": "^2.4.5",
    "@choc-ui/chakra-autocomplete": "^5.1.4",
    "@tanstack/match-sorter-utils": "^8.7.2",
    "@tanstack/react-table": "^8.7.4",
    "@tanstack/table-core": "^8.7.9",
    "@vitejs/plugin-react": "^3.1.0",
    "@vitejs/plugin-react-swc": "^3.2.0",
    "autoprefixer": "^10.4.13",
    "chakra-ui-steps": "^1.8.0",
    "common": "*",
    "currency.js": "^2.0.4",
    "database": "*",
    "date-fns": "^2.29.3",
    "i18next": "^22.0.4",
    "i18next-browser-languagedetector": "^7.0.1",
    "i18next-http-backend": "^2.0.1",
    "katex": "^0.16.4",
    "next": "^13.1.1",
    "react": "^18.2.0",
    "react-beautiful-dnd": "latest",
    "react-dnd": "latest",
    "react-dnd-html5-backend": "latest",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.41.2",
    "react-i18next": "^12.0.0",
    "react-icons": "^4.7.1",
    "react-markdown": "^8.0.4",
    "rehype-katex": "^6.0.2",
    "remark-gfm": "^3.0.1",
    "remark-math": "^5.1.1",
    "tsup": "~6.6.3",
    "vite": "^4.1.4",
    "vite-plugin-svgr": "^2.4.0",
    "vite-tsconfig-paths": "^4.0.8",
    "zod": "^3.19.1"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^11.0.0",
    "@storybook/builder-vite": "^0.4.2",
    "@storybook/react": "^6.5.15",
    "@storybook/testing-library": "^0.0.13",
    "@types/jest": "29.2.2",
    "@types/react": "^17.0.13",
    "@types/react-dom": "^17.0.8",
    "@typescript-eslint/eslint-plugin": "^5.51.0",
    "@typescript-eslint/parser": "^5.52.0",
    "eslint": "^8.34.0",
    "eslint-config-next": "12.3.4",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "7.32.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-sonarjs": "0.16.0",
    "jest": "^26.6.3",
    "postcss": "^8.4.20",
    "rimraf": "^3.0.2",
    "tsconfig": "*",
    "tsx": "^3.7.1",
    "typescript": "5.0.3"
  },
  "packageManager": "yarn@3.5.0"
}
