{
  "name": "ui",
  "version": "2.0.0",
  "private": true,
  "homepage": "",
  "repository": {
    "type": "git",
    "url": "https://github.com/HarrisFauntleroy/alchemical-finance"
  },
  "license": "MIT",
  "author": {
    "name": "Harris Fauntleroy",
    "url": "https://github.com/HarrisFauntleroy"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "build-storybook": "storybook build",
    "clean": "rimraf .turbo node_modules dist tsconfig.tsbuildinfo",
    "dev": "concurrently \"yarn build --watch\" \"yarn storybook\"",
    "lint": "yarn esw . --ext .js,.jsx,.ts,.tsx --color",
    "lint:fix": "yarn esw . --ext .js,.jsx,.ts,.tsx --fix --color",
    "lint:watch": "yarn esw . --ext .js,.jsx,.ts,.tsx --watch --fix --color --changed --clear",
    "preview": "vite preview",
    "start": "NODE_ENV=production node dist/index.js",
    "storybook": "storybook dev -p 6006",
    "storybook:build": "storybook build",
    "storybook:serve": "http-server storybook-static --port 6006 --silent",
    "test": "jest --passWithNoTests",
    "test:ci": "yarn test --ci --coverage --maxWorkers=2",
    "test:watch": "yarn test --watchAll",
    "type-check": "tsc --noEmit",
    "watch": "yarn type-check --watch"
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "es6": true,
      "jest": true,
      "node": true
    },
    "parser": "@typescript-eslint/parser",
    "settings": {
      "import/resolver": {
        "typescript": true,
        "node": true
      },
      "react": {
        "version": "detect"
      }
    },
    "plugins": [
      "@typescript-eslint",
      "simple-import-sort",
      "import",
      "sonarjs",
      "jsx-a11y",
      "prettier"
    ],
    "extends": [
      "next",
      "plugin:@typescript-eslint/recommended",
      "eslint:recommended",
      "plugin:import/typescript",
      "plugin:import/recommended",
      "plugin:sonarjs/recommended",
      "plugin:jsx-a11y/recommended",
      "plugin:storybook/recommended",
      "plugin:prettier/recommended"
    ],
    "rules": {
      "@typescript-eslint/no-unused-vars": [
        "error",
        {
          "argsIgnorePattern": "^_"
        }
      ],
      "import/extensions": "off",
      "import/no-extraneous-dependencies": [
        "error",
        {
          "devDependencies": [
            "**/*.stories.*",
            "**/.storybook/**/*.*"
          ],
          "peerDependencies": true
        }
      ],
      "simple-import-sort/imports": [
        "off",
        {
          "groups": [
            [
              "^react$"
            ],
            [
              "^(ui|common|database|database/generated/prisma-client)$"
            ],
            [
              "^[./]"
            ]
          ]
        }
      ],
      "@next/next/no-html-link-for-pages": "off"
    },
    "ignorePatterns": [
      "dist"
    ]
  },
  "jest": {
    "testEnvironment": "node",
    "testRegex": ".test.*",
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    }
  },
  "dependencies": {
    "@chakra-ui/react": "2.6.1",
    "@choc-ui/chakra-autocomplete": "^5.1.5",
    "@emotion/react": "^11.11.0",
    "@emotion/styled": "^11.11.0",
    "@mantine/carousel": "^6.0.11",
    "@mantine/core": "^6.0.11",
    "@mantine/dates": "^6.0.11",
    "@mantine/dropzone": "^6.0.11",
    "@mantine/form": "^6.0.11",
    "@mantine/hooks": "^6.0.11",
    "@mantine/modals": "^6.0.11",
    "@mantine/notifications": "^6.0.11",
    "@mantine/nprogress": "^6.0.11",
    "@mantine/prism": "^6.0.11",
    "@mantine/spotlight": "^6.0.11",
    "@mantine/tiptap": "^6.0.11",
    "@phosphor-icons/react": "^2.0.9",
    "@tabler/icons-react": "^2.20.0",
    "@tanstack/match-sorter-utils": "^8.8.4",
    "@tanstack/react-table": "^8.9.1",
    "@tanstack/table-core": "^8.9.1",
    "@tiptap/extension-link": "^2.0.3",
    "@tiptap/react": "^2.0.3",
    "@tiptap/starter-kit": "^2.0.3",
    "autoprefixer": "^10.4.14",
    "chakra-ui-steps": "^1.8.0",
    "common": "*",
    "currency.js": "^2.0.4",
    "database": "*",
    "date-fns": "^2.30.0",
    "dayjs": "^1.11.7",
    "embla-carousel-react": "^8.0.0-rc05",
    "i18next": "^22.0.4",
    "i18next-browser-languagedetector": "^7.0.1",
    "i18next-http-backend": "^2.2.1",
    "katex": "^0.16.7",
    "next": "^13.4.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.41.2",
    "react-i18next": "^12.0.0",
    "react-icons": "^4.9.0",
    "react-markdown": "^8.0.7",
    "rehype-katex": "^6.0.3",
    "remark-gfm": "^3.0.1",
    "remark-math": "^5.1.1",
    "tsup": "^6.7.0",
    "zod": "3.21.4"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.22.2",
    "@babel/preset-react": "^7.22.3",
    "@babel/preset-typescript": "^7.21.5",
    "@jest/types": "29.5.0",
    "@storybook/addon-essentials": "^7.0.18",
    "@storybook/addon-interactions": "^7.0.18",
    "@storybook/addon-links": "^7.0.18",
    "@storybook/addon-mdx-gfm": "^7.0.18",
    "@storybook/blocks": "^7.0.18",
    "@storybook/nextjs": "^7.0.18",
    "@storybook/react": "^7.0.18",
    "@storybook/react-webpack5": "^7.0.18",
    "@storybook/testing-library": "^0.1.0",
    "@types/jest": "^29.5.1",
    "@types/node": "^18.16.16",
    "@types/react": "18.2.7",
    "@types/react-dom": "^18.2.4",
    "@typescript-eslint/eslint-plugin": "^5.59.7",
    "@typescript-eslint/parser": "^5.59.7",
    "concurrently": "^8.0.1",
    "eslint": "^8.38.0",
    "eslint-config-next": "13.4.4",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-sonarjs": "0.19.0",
    "eslint-plugin-storybook": "^0.6.12",
    "eslint-watch": "^8.0.0",
    "jest": "29.5.0",
    "postcss": "^8.4.24",
    "rimraf": "^4.4.1",
    "storybook": "^7.0.18",
    "storybook-dark-mode": "^3.0.0",
    "ts-jest": "^29.1.0",
    "tsx": "^3.12.7",
    "typescript": "5.0.4"
  },
  "packageManager": "yarn@3.5.1"
}
