{
	"name": "ui",
	"version": "1.0.1",
	"private": true,
	"license": "MIT",
	"sideEffects": false,
	"main": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"files": [
		"dist/**"
	],
	"scripts": {
		"build": "tsup src/index.ts --format esm,cjs --dts --external react",
		"build-storybook": "build-storybook",
		"clean": "rimraf .turbo node_modules dist",
		"dev": "start-storybook -p 6006",
		"format": "prettier --write .",
		"lint": "next lint",
		"lint:fix": "eslint . --fix",
		"start": "NODE_ENV=production ts-node dist/index.js",
		"storybook": "start-storybook -p 6006",
		"test": "jest --passWithNoTests",
		"test:ci": "yarn test --ci --coverage --maxWorkers=2",
		"test:watch": "yarn test --watchAll",
		"type-check": "tsc --noEmit"
	},
	"jest": {
		"moduleDirectories": [
			"node_modules",
			"src"
		],
		"moduleFileExtensions": [
			"js",
			"json",
			"ts"
		],
		"roots": [
			"src"
		],
		"testEnvironment": "node",
		"testRegex": ".test.ts$",
		"transform": {
			"^.+\\.(t|j)s$": "ts-jest"
		}
	},
	"dependencies": {
		"@chakra-ui/react": "^2.4.5",
		"@chakra-ui/storybook-addon": "^4.0.16",
		"@choc-ui/chakra-autocomplete": "^5.1.4",
		"@tanstack/match-sorter-utils": "^8.7.2",
		"@tanstack/react-table": "^8.7.4",
		"autoprefixer": "^10.4.13",
		"chakra-ui-steps": "^1.8.0",
		"common": "*",
		"currency.js": "^2.0.4",
		"database": "*",
		"katex": "^0.16.4",
		"next": "^13.1.1",
		"prisma": "*",
		"react": "^18.2.0",
		"react-beautiful-dnd": "*",
		"react-dnd": "*",
		"react-dnd-html5-backend": "*",
		"react-dom": "^18.2.0",
		"react-hook-form": "^7.41.2",
		"react-icons": "^4.7.1",
		"react-markdown": "^8.0.4",
		"rehype-katex": "^6.0.2",
		"remark-gfm": "^3.0.1",
		"remark-math": "^5.1.1",
		"tsup": "^6.5.0",
		"zod": "^3.19.1"
	},
	"devDependencies": {
		"@chakra-ui/storybook-addon": "^4.0.16",
		"@storybook/addon-actions": "^6.5.15",
		"@storybook/addon-essentials": "^6.5.15",
		"@storybook/addon-interactions": "^6.5.15",
		"@storybook/addon-links": "^6.5.15",
		"@storybook/addon-toolbars": "^6.5.16",
		"@storybook/builder-webpack4": "^6.5.15",
		"@storybook/manager-webpack4": "^6.5.15",
		"@storybook/react": "^6.5.15",
		"@storybook/testing-library": "^0.0.13",
		"@types/jest": "29.2.2",
		"@types/react": "^17.0.13",
		"@types/react-dom": "^17.0.8",
		"config": "*",
		"eslint": "^8.12.0",
		"eslint-plugin-react": "^7.31.11",
		"eslint-plugin-storybook": "^0.6.8",
		"jest": "^26.6.3",
		"postcss": "^8.4.20",
		"rimraf": "^3.0.2",
		"tsconfig": "*",
		"tsx": "^3.7.1",
		"typescript": "4.7.4"
	},
	"packageManager": "yarn@1.22.19"
}
