{
  "name": "ui",
  "version": "1.1.0",
  "private": true,
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "clean": "rimraf .turbo node_modules dist tsconfig.tsbuildinfo",
    "dev": "concurrently \"yarn build --watch\"",
    "lint": "yarn eslint .",
    "lint:fix": "yarn esw . --watch --fix",
    "preview": "vite preview",
    "start": "NODE_ENV=production node dist/index.js",
    "storybook": "storybook dev -p 6006",
    "storybook:build": "storybook build",
    "storybook:serve": "http-server storybook-static --port 6006 --silent",
    "test": "jest --passWithNoTests",
    "test:ci": "yarn test --ci --coverage --maxWorkers=2",
    "test:watch": "yarn test --watchAll",
    "type-check": "tsc --noEmit"
  },
  "jest": {
    "testEnvironment": "node",
    "testRegex": ".test.*",
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    }
  },
  "dependencies": {
    "@chakra-ui/react": "2.5.5",
    "@choc-ui/chakra-autocomplete": "^5.1.4",
    "@emotion/react": "11.10.6",
    "@emotion/styled": "^11.10.6",
    "@tanstack/match-sorter-utils": "^8.7.2",
    "@tanstack/react-table": "^8.7.4",
    "@tanstack/table-core": "^8.7.9",
    "autoprefixer": "^10.4.13",
    "chakra-ui-steps": "^1.8.0",
    "common": "*",
    "currency.js": "^2.0.4",
    "database": "*",
    "date-fns": "^2.29.3",
    "i18next": "^22.0.4",
    "i18next-browser-languagedetector": "^7.0.1",
    "i18next-http-backend": "^2.0.1",
    "katex": "^0.16.4",
    "next": "^13.1.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.41.2",
    "react-i18next": "^12.0.0",
    "react-icons": "^4.7.1",
    "react-markdown": "^8.0.4",
    "rehype-katex": "^6.0.2",
    "remark-gfm": "^3.0.1",
    "remark-math": "^5.1.1",
    "zod": "3.21.1"
  },
  "devDependencies": {
    "@jest/types": "29.4.0",
    "@storybook/react": "^6.5.15",
    "@storybook/testing-library": "^0.0.13",
    "@types/jest": "^29.5.0",
    "@types/node": "^18.11.6",
    "@types/react": "18.0.33",
    "@types/react-dom": "^18.0.11",
    "@typescript-eslint/eslint-plugin": "^5.51.0",
    "@typescript-eslint/parser": "^5.52.0",
    "concurrently": "^8.0.1",
    "eslint": "^8.38.0",
    "eslint-config-next": "13.1.6",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-sonarjs": "0.16.0",
    "eslint-plugin-storybook": "^0.6.11",
    "eslint-watch": "^8.0.0",
    "jest": "29.4.3",
    "postcss": "^8.4.20",
    "rimraf": "^4.4.1",
    "storybook": "^7.0.2",
    "ts-jest": "^29.1.0",
    "tsup": "^6.7.0",
    "tsx": "^3.7.1",
    "typescript": "4.9.5"
  },
  "packageManager": "yarn@3.5.0"
}
