{
  "name": "common",
  "version": "2.0.0",
  "private": true,
  "homepage": "",
  "repository": {
    "type": "git",
    "url": "https://github.com/HarrisFauntleroy/alchemical-finance"
  },
  "license": "MIT",
  "author": {
    "name": "Harris Fauntleroy",
    "url": "https://github.com/HarrisFauntleroy"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "yarn build --watch",
    "format": "prettier --write .",
    "lint": "eslint . --color",
    "lint:fix": "eslint . --fix --color",
    "lint:watch": "esw . --ext .js,.jsx,.ts,.tsx --watch --fix --color --changed --clear",
    "start": "NODE_ENV=production node dist/index.js",
    "test": "jest --passWithNoTests",
    "test:ci": "yarn test --ci --coverage --maxWorkers=2",
    "test:watch": "yarn test --watchAll",
    "type-check": "tsc --noEmit",
    "watch": "yarn type-check --watch"
  },
  "jest": {
    "coverageThreshold": {
      "global": {
        "lines": 80
      }
    },
    "testEnvironment": "node",
    "testRegex": ".test.*",
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    }
  },
  "dependencies": {
    "currency.js": "^2.0.4",
    "database": "*",
    "zod": "3.21.4"
  },
  "devDependencies": {
    "@jest/types": "29.5.0",
    "@types/jest": "^29.5.2",
    "@types/node": "^18.16.17",
    "@types/react": "18.2.11",
    "@types/react-dom": "^18.2.4",
    "@typescript-eslint/eslint-plugin": "^5.59.9",
    "@typescript-eslint/parser": "^5.59.9",
    "eslint": "^8.42.0",
    "eslint-config-next": "13.4.5",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-sonarjs": "0.19.0",
    "eslint-watch": "^8.0.0",
    "jest": "29.5.0",
    "rimraf": "^4.4.1",
    "ts-jest": "^29.1.0",
    "tsup": "^6.7.0",
    "tsx": "^3.12.7",
    "typescript": "5.1.3"
  },
  "packageManager": "yarn@3.5.1"
}
