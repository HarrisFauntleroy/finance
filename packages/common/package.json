{
  "name": "common",
  "version": "2.0.0",
  "private": true,
  "homepage": "",
  "repository": {
    "type": "git",
    "url": "https://github.com/HarrisFauntleroy/alchemical-finance"
  },
  "license": "MIT",
  "author": {
    "name": "Harris Fauntleroy",
    "url": "https://github.com/HarrisFauntleroy"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "yarn build --watch",
    "start": "NODE_ENV=production node dist/index.js",
    "test": "jest --passWithNoTests",
    "test:ci": "yarn test --ci --coverage --maxWorkers=2",
    "test:watch": "yarn test --watchAll",
    "type-check": "tsc --noEmit",
    "watch": "yarn type-check --watch"
  },
  "jest": {
    "coverageThreshold": {
      "global": {
        "lines": 80
      }
    },
    "testEnvironment": "node",
    "testRegex": ".test.*",
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    }
  },
  "dependencies": {
    "currency.js": "^2.0.4",
    "database": "*",
    "zod": "3.21.4"
  },
  "devDependencies": {
    "@jest/types": "29.5.0",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.10.5",
    "@types/react": "18.2.13",
    "@types/react-dom": "^18.2.6",
    "jest": "29.5.0",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.1.0",
    "tsup": "^8.0.1",
    "tsx": "^4.7.0",
    "typescript": "5.0.3"
  },
  "packageManager": "yarn@4.0.2"
}
