{
	"compilerOptions": {
		"strict": true,
		"useUnknownInCatchVariables": true,
		"skipLibCheck": true,
		"forceConsistentCasingInFileNames": true,
		"esModuleInterop": true,
		"moduleResolution": "node",
		"resolveJsonModule": true,
		"isolatedModules": true,
		"incremental": true,
		"newLine": "lf"
	},
	"exclude": ["**/node_modules", "**/dist"]
}
